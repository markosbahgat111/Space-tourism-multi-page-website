{
  "compilerOptions": {
      "target": "es6",
      "module": "esnext",
      "allowSyntheticDefaultImports": true,
      "baseUrl": "src",
      "paths": {
          "~/*":["./src/*"]
      },
      "allowJs": true,
      "strict": true,
      "jsx": "react-jsx",
      "noEmit": true
  },
  "include": ["src"]
}